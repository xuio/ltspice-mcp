      "description": "AC low-pass testbench with Vin, R, and C.",
      "type": "netlist",
      "circuit_name": "rc_lowpass_ac",
      "sheet_width": 1200,
      "sheet_height": 900,
      "parameter_defaults": {
        "vin_ac": "1",
        "r_value": "1k",
        "c_value": "100n",
        "ac_points": "30",
        "f_start": "10",
        "f_stop": "1e6"
      },
      "netlist_content": "* RC low-pass template\nV1 in 0 AC {vin_ac}\nR1 in out {r_value}\nC1 out 0 {c_value}\n.ac dec {ac_points} {f_start} {f_stop}\n.op\n.end"
